    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "iot_camera_sensor_app",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define-from-file=./env/.env",
            ],
        },
        {
            "name": "iot_camera_sensor_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define-from-file=./env/.env",
            ],
        },
        {
            "name": "iot_camera_sensor_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define-from-file=./env/.env",
            ],
        }
    ]
}